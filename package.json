{
  "name": "conventional-changelog-cmyr-config",
  "version": "1.2.0-beta",
  "description": "This preset extends the angular preset",
  "main": "index.js",
  "scripts": {
    "commit": "git add . && git-cz",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0 -n ./index.js",
    "release": "release-it",
    "test-windows": "mocha --timeout 30000",
    "deploy-beta": "npm publish --tag beta",
    "deploy": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/conventional-changelog-cmyr-config.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [
    "conventional-changelog",
    "angular",
    "preset",
    "changelog",
    "emoji"
  ],
  "author": "CaoMeiYouRen <caomeiyouren@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/conventional-changelog-cmyr-config/issues"
  },
  "homepage": "https://github.com/CaoMeiYouRen/conventional-changelog-cmyr-config#readme",
  "devDependencies": {
    "better-than-before": "^1.0.0",
    "chai": "^4.2.0",
    "commitizen": "^3.1.1",
    "compare-func": "^1.3.2",
    "conventional-changelog-cli": "^2.0.21",
    "conventional-changelog-core": "^3.2.2",
    "coveralls": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "debug": "^4.1.1",
    "defaults": "^1.0.3",
    "git-dummy-commit": "^1.3.0",
    "github-url-from-git": "^1.5.0",
    "istanbul": "^0.4.5",
    "jscs": "^3.0.7",
    "jshint": "^2.10.2",
    "mocha": "*",
    "q": "^1.5.1",
    "shelljs": "^0.8.3",
    "through2": "^3.0.1"
  },
  "changelog": {
    "language": "zh"
  }
}